class Solution {

    void printNos(int N) {
        if(N<1)
        return;
        printNos(N-1);
        System.out.print(N+" ");
    }
}

or
 public static void fun(int i,int n)
    {
        if(i>n)
        {
            return;
        }
        System.out.print(i+" ");
        fun(i+1,n);
    }
